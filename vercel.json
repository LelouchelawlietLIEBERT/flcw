{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    },
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "models.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/contacts",
      "dest": "/main.py"
    },
    {
      "src": "/create_contact",
      "dest": "/main.py"
    },
    {
      "src": "/update_contact/(.*)",
      "dest": "/main.py"
    },
    {
      "src": "/delete_contact/(.*)",
      "dest": "/main.py"
    }
  ]
}
